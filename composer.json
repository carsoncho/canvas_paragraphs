{
    "name": "carsoncho/canvas_paragraphs",
    "description": "A monorepo that holds several paragraph type Drupal recipes.",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "recipes/*"
        }
    ],
    "authors": [
        {
            "name": "Carson Oldson",
            "email": "coldson@forumone.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "carsoncho/accordion_paragraph": "dev-main",
        "carsoncho/block_embed_paragraph": "dev-main"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

